{
	"manifest_version" : 2,
	"name" : "Openload Only Video",
	"short_name" : "openload_video",
	"version" : "1.0",
	"author" : "...",
	"description" : "Exctract direct URL for openload videos",
	"content_scripts": [
		{
			"all_frames": true,
			"matches": ["*://*/*"],
			"js": ["node_modules/jquery/dist/jquery.min.js", "js/openload.js", "js/torjackan.js"],
			"run_at": "document_end"
		}
	],
	"icons" : {
		"32" : "icons/32x32.png",
		"48" : "icons/48x48.png"
	},
	
	"background" : {
		"scripts" : ["hot-reload.js","background.js"],
		"persistent" : false
	},
	
    "browser_action": {
        "default_title": "...",
        "default_icon": "icons/48x48.png",
		"default_popup": "popup.html"
    }
}



{
	"manifest_version" : 2,
	"name" : "MY_EXTENSION",
	"short_name" : "openload_video",
	"version" : "1.02",
	"author" : "Evgeniy Ryzhov",
	"description" : "Extract openload video; Torjackan justifier; Cross-browser session saver",
	"content_scripts": [
		{
			"all_frames": true,
			"matches": ["*://openload.co/*"],
			"js": ["js/inject/openload/openload.js"],
			"run_at": "document_end"
		},		
		{
			"all_frames": true,
			"matches": ["*://*.oloadcdn.net/*"],
			"js": ["js/inject/openload/video.js"],
			"css": ["style/video.css"],
			"run_at": "document_end"
		}
	],
	"icons" : {
		"32" : "icons/32x32.png",
		"48" : "icons/48x48.png"
	},
	
	"background" : {
		"scripts" : [
			"js/background/background.js"
		],
		"persistent" : false
	},
	"permissions": [
		"storage",
		"tabs",
		"bookmarks",
		"identity"
	],
    "browser_action": {
        "default_title": "Click to open Popup window.",
        "default_icon": "icons/48x48.png",
		"default_popup": "popup.html"
	},
	"key": ""
}


